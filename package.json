{
  "name": "Sfotipy",
  "version": "1.0.0",
  "description": "Musical Web App. Listen all the music you love, everywhere.",
  "main": "server.js",
  "keywords": [
    "Sfotipy",
    "Musical Web App",
    "Music",
  	"Node.js",
  	"Backbone.js"
  ],
  "scripts": {
    "bundle": "browserify app/js/app.js -o app/js/sfotipy.js",
    "watch-js": "watchify app/js/app.js -o app/js/sfotipy.js --debug --verbose",
    "watch-styl": "stylus -u nib -w -c -o app/css/ app/stylus/estilos.styl",
	  "start": "node server.js"
  },
  "author": "Oliver Reyes <olimitch7@gmail.com>",
  "license": "ISC",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "Gulpfile.js"
  ],
  "dependencies": {
    "backbone": "^1.1.2",
    "express": "^4.11.2",
    "handlebars": "^3.0.0",
    "jquery": "^3.4.1",
    "node-static": "^0.7.6",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "gulp": "^3.8.11",
    "gulp-browserify": "^0.5.1",
    "gulp-changed": "^0.2.1",
    "gulp-concat": "^2.2.0",
    "gulp-imagemin": "^0.1.5",
    "gulp-minify-css": "^0.3.0",
    "gulp-minify-html": "^0.1.1",
    "gulp-strip-debug": "^0.3.0",
    "gulp-uglify": "^0.2.1",
    "watchify": "^2.3.0"
  }
}
